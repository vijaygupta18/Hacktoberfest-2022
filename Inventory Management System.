        "prodID=str(input(\"Enter the Product - Id= \"))\n",

        "quantity=int(input(\"Enter the Quantity= \"))\n",

        "\n",

        "if(quantity<records[prodID]['Quantity']):\n",

        "  print(\"*****************************\")\n",

        "  print(\"Hey, Welcome!! \\nYou Chose--\\n\")\n",

        "  print(\"Product ID= \",prodID)\n",

        "  print(\"Product Name= \", records[prodID]['Name'])\n",

        "  print(\"Product Amount= \", records[prodID]['Price'])\n",

        "  print(\"Manufacturing Date= \", records[prodID]['Manufacturing Date'])\n",

        "  print(\"Expiry Date= \", records[prodID]['Expiry Date'])\n",

        "  print(\"Available Quantities= \",records[prodID]['Quantity'])\n",

        "  print(\"Discount= \",str(records[prodID]['Discount']) + \" %\")\n",

        "  print(\"****************************\\n\")\n",

        "  totalDis=int(records[prodID]['Discount'])*quantity\n",

        "  dis=totalDis*int(records[prodID]['Price'])*quantity*0.01\n",

        "  print(\"Amount= \",(records[prodID]['Price'])*quantity)\n",

        "  print(\"Total Discount you got= \",str(quantity*int(records[prodID]['Discount'])) + \" %\")\n",

        "  print(\"Discount Amount= \",dis)\n",

        "  print(\"****************************\")\n",

        "  print(\"\\nTotal Amount to be paid= \", str(records[prodID]['Price']*quantity-dis)  + \" /-\")\n",

        "\n",

        "  records[prodID]['Quantity'] = records[prodID]['Quantity']-quantity\n",

        "else:\n",

        "  print(\"*****************************\")\n",

        "  print(\"Hey, Welcome!! \\nYou Chose--\\n\")\n",

        "  print(\"Product ID= \",prodID)\n",

        "  print(\"Product Name= \", records[prodID]['Name'])\n",

        "  print(\"Product Amount= \", records[prodID]['Price'])\n",

        "  print(\"Manufacturing Date= \", records[prodID]['Manufacturing Date'])\n",

        "  print(\"Expiry Date= \", records[prodID]['Expiry Date'])\n",

        "  print(\"Available Quantities= \",records[prodID]['Quantity'])\n",

        "  print(\"Discount= \",str(records[prodID]['Discount']) + \" %\")\n",

        "  print(\"******************************\\n\")\n",

        "  print(\"OOps, no sufficient quantities of the selected item are available.\")"

